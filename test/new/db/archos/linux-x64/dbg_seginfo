NAME='Can set db on main'
FILE=../bins/elf/analysis/hello-objc-linux
ARGS=-d
CMDS=<<EOF
db main;db~1[14];
EOF
EXPECT=<<EOF
"main"
EOF
RUN

NAME='Code seg is sane'
FILE=../bins/elf/analysis/hello-objc-linux
BROKEN=1
ARGS=-d
CMDS=<<EOF
iS~LOAD0
EOF
EXPECT=<<EOF
idx=32 vaddr=0x08048000 paddr=0x00000000 sz=2848 vsz=2848 perm=m-r-x name=LOAD0
EOF
RUN
